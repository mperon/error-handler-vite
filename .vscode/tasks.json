{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "Launch Frontend",
      "type": "yarn",
      "task": "dev",
      "isBackground": true,
      "problemMatcher": [
        {
          "base": "$tsc-watch",
          "background": {
